{
  "name": "youtube-downloader",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:ssr": "ng build && ng run youtube-downloader:ssr",
    "serve:ssr": "node dist/youtube-downloader/server/main.js",
    "test": "ng test",
    "serve:ssr:youtube-downloader": "node dist/youtube-downloader/server/server.mjs",
    "build:client": "ng build --configuration production",
    "build:server": "ng run youtube-downloader:ssr:production",
    "start:ssr": "node dist/server/server.js",
    "watch:client": "ng build --configuration development --watch",
    "watch:server": "ng run youtube-downloader:ssr:development --watch",
    "dev:ssr": "concurrently \"npm run watch:client\" \"npm run watch:server\" \"nodemon dist/server/server.js\"",
    "start": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/cdk": "^19.1.1",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/material": "^19.1.1",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/platform-server": "^19.1.3",
    "@angular/router": "^19.1.3",
    "@angular/ssr": "^19.1.4",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "express": "^4.21.2",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "katex": "^0.16.22",
    "ngx-infinite-scroll": "^19.0.0",
    "ngx-markdown": "^19.1.1",
    "ngx-markdown-editor": "^5.3.4",
    "nodemon": "^3.1.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.3",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.6.2"
  }
}
